    "/reservation/seats": {
      "post": {
        "operationId": "ReservationController_reserveSeats",
        "summary": "좌석 예약 요청",
        "parameters": [
          {
            "name": "participation-token",
            "in": "header",
            "description": "Participation token",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "authorization",
            "in": "header",
            "description": "Authorization token",
            "required": true,
            "schema": {
              "type": "string"
