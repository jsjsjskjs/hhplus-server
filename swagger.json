  "components": {
    "schemas": {
      "OkResponseDto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "example": "ok"
          }
        },
        "required": [
          "message"
        ]
      },
      "GetEntriesResDto": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "WAITING",
              "ATTENDING"
            ]
          },
          "nowNumber": {
            "type": "number"
          },
          "token": {
            "type": "string"
          }
        },
        "required": [
          "type",
          "nowNumber"
        ]
      },
      "Client": {
        "type": "object",
        "properties": {
          "createdAt": {
            "type": "string"
          },
          "updatedAt": {
            "type": "string"
          },
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "firstName": {
            "type": "string",
            "format": "string"
          },
          "lastName": {
            "type": "string",
            "format": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "point": {
            "type": "number",
            "default": 0
          },
          "password": {
            "type": "string",
            "format": "string"
          }
        },
        "required": [
          "createdAt",
          "updatedAt",
          "id",
          "firstName",
          "lastName",
          "email",
          "point",
          "password"
        ]
      },
      "SeatGrade": {
        "type": "object",
        "properties": {
          "createdAt": {
            "type": "string"
          },
          "updatedAt": {
            "type": "string"
          },
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string",
            "enum": [
              "S",
              "A",
              "B",
              "C"
            ]
          },
          "price": {
            "type": "number"
          }
        },
        "required": [
          "createdAt",
          "updatedAt",
          "id",
          "name",
          "price"
        ]
      },
      "Concert": {
        "type": "object",
        "properties": {
          "createdAt": {
            "type": "string"
          },
          "updatedAt": {
            "type": "string"
          },
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string",
            "format": "string"
          },
          "artist": {
            "type": "string",
            "format": "string"
          },
          "venue": {
            "type": "string",
            "format": "string"
          },
          "description": {
            "type": "string",
            "format": "string"
          }
        },
        "required": [
          "createdAt",
          "updatedAt",
          "id",
          "name",
          "artist",
          "venue",
          "description"
        ]
      },
      "ConcertDates": {
        "type": "object",
        "properties": {
          "createdAt": {
            "type": "string"
          },
          "updatedAt": {
            "type": "string"
          },
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "concertDate": {
            "type": "timestamp",
            "nullable": false
          },
          "reserveDate": {
            "type": "timestamp",
            "nullable": false
          },
          "concert": {
            "$ref": "#/components/schemas/Concert"
          }
        },
        "required": [
          "createdAt",
          "updatedAt",
          "id",
          "concertDate",
          "reserveDate",
          "concert"
        ]
      },
      "Seat": {
        "type": "object",
        "properties": {
          "createdAt": {
            "type": "string"
          },
          "updatedAt": {
            "type": "string"
          },
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "section": {
            "type": "string",
            "format": "string"
          },
          "row": {
            "type": "string",
            "format": "string"
          },
          "number": {
            "type": "number"
          },
          "isBooked": {
            "type": "boolean",
            "default": false
          },
          "seatGrade": {
            "$ref": "#/components/schemas/SeatGrade"
          },
          "concertDates": {
            "$ref": "#/components/schemas/ConcertDates"
          }
        },
        "required": [
          "createdAt",
          "updatedAt",
          "id",
          "section",
          "row",
          "number",
          "isBooked",
          "seatGrade",
          "concertDates"
        ]
      },
      "Reservation": {
        "type": "object",
        "properties": {
          "createdAt": {
            "type": "string"
          },
          "updatedAt": {
            "type": "string"
          },
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "address": {
            "type": "string",
            "format": "string"
          },
          "status": {
            "type": "string",
            "enum": [
              "DONE",
              "CANCEL"
            ]
          },
          "client": {
            "$ref": "#/components/schemas/Client"
          },
          "seat": {
            "$ref": "#/components/schemas/Seat"
          }
        },
        "required": [
          "createdAt",
          "updatedAt",
          "id",
          "address",
          "status",
          "client",
          "seat"
        ]
      },
      "CoinLog": {
        "type": "object",
        "properties": {
          "createdAt": {
            "type": "string"
          },
          "updatedAt": {
            "type": "string"
          },
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "eventType": {
            "type": "string",
            "enum": [
              "CHARGE",
              "REDUCE",
              "REFUND"
            ]
          },
          "point": {
            "type": "number",
            "default": 0
          },
          "client": {
            "$ref": "#/components/schemas/Client"
          },
          "reservation": {
            "$ref": "#/components/schemas/Reservation"
          }
        },
        "required": [
          "createdAt",
          "updatedAt",
          "id",
          "eventType",
          "point",
          "client"
        ]
      },
      "PostReservationSeat400Dto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "example": "해당 좌석은 이미 예매 중 입니다"
          },
          "error": {
            "type": "string",
            "example": "Bad Request"
          },
          "status": {
            "type": "number",
            "example": 400
          }
        },
        "required": [
          "message",
          "error",
          "status"
        ]
      },
      "PostConfirmReservation400Dto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "example": "보유하고 있는 Coin이 부족합니다"
          },
          "error": {
            "type": "string",
            "example": "Bad Request"
          },
          "status": {
            "type": "number",
            "example": 400
          }
        },
        "required": [
          "message",
          "error",
          "status"
        ]
      }
    }
  }
}